{
	"name" : "tibemolde/placeholder-pixel",
	"description" : "Create a base64-encoded transparent pixel of the given width and height",
	"type" : "craft-plugin",
	"keywords" : [
		"craft",
		"cms",
		"craftcms",
		"craft-plugin",
		"placeholder pixel"
	],
	"support" : {
		"docs" : "https://github.com/tibemolde/craft3-placeholderpixel/blob/master/README.md",
		"issues" : "https://github.com/tibemolde/craft3-placeholderpixel/issues"
	},
	"license" : "MIT",
	"authors" : [
		{
			"name" : "TIBE Molde",
			"homepage" : "https://github.com/tibemolde"
		}
	],
	"require" : {
		"roave/security-advisories" : "dev-master",
		"craftcms/cms" : "^3.0.0-RC1"
	},
	"autoload" : {
		"psr-4" : {
			"tibemolde\\placeholderpixel\\" : "src/"
		}
	},
	"extra" : {
		"name" : "Placeholder Pixel",
		"handle" : "placeholder-pixel",
		"schemaVersion" : "1.0.0",
		"hasCpSettings" : false,
		"hasCpSection" : false,
		"changelogUrl" : "https://raw.githubusercontent.com/tibemolde/placeholder-pixel/master/CHANGELOG.md",
		"components" : {
			"placeholder" : "tibemolde\\placeholderpixel\\services\\PixelService"
		},
		"class" : "tibemolde\\placeholderpixel\\PlaceholderPixel"
	}
}
